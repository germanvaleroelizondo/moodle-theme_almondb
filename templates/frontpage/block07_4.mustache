<div class="block07desing4 container">
    <div class="row"> 
        {{#block07}}
        <div class="col-md-4 col-lg-3 col-sm-12 mb-4">
            <div class="card h-100" >
                <div class="single-course" style = "background-image: url({{imgurl}});">
                {{#block07priceshow}}
                    <div class="enrol">
                        {{cost}} {{currency}}
                    </div>
                {{/block07priceshow}}
                </div>
                <div class="card-body">
                    <small>
                        <a href="{{categorylink}}" class="text-decoration-none"><i class="fa fa-chevron-circle-right"> </i> {{categoryName}}</a>
                    </small>
                    <h6 class="card-title post-heading"><a href="{{courselink}}" class="text-decoration-none">
                        {{#block07shortname}}{{shortname}}{{/block07shortname}}
                        {{#block07fullname}}{{fullname}}{{/block07fullname}}</a>
                    </h6>
                </div>
                <div class="card-footer bg-white">
                    <small class="hytext">Updated {{update}}</small>
                </div>
            </div>
        </div>
        {{/block07}} 
    </div>
</div>  
